# Server configuration file, see
# http://roundup.sourceforge.net/docs/admin_guide.html for details

[main]

port = 8088
host = localhost

user = roundup

# Please don't disable the pid file:
pidfile = /var/run/roundup/server.pid
logfile = /var/log/roundup/roundup.log

[trackers]

<%= @tracker_name %> = /var/lib/roundup/trackers/<%= @tracker_name %>
