# Puppet: Adblockplus::Log::Rotation['<%= @title %>']
<%= @path %> {
  <%= @interval %>
  compress
  missingok
  nodateext
  rotate <%= @count.to_i %>
  sharedscripts
  shred
<% unless @postrotate.empty? -%>
  postrotate
    <%= @postrotate.join("\n    ") rescue @postrotate %>
  endscript
<% end -%>
}
